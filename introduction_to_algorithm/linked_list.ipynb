   "source": [
    "mutable struct Node\n",
    "    prev::Union{Node,Nothing}\n",
    "    key\n",
    "    next::Union{Node,Nothing}\n",
    "end\n",
    "\n",
    "\n",
    "mutable struct LinkedList\n",
    "    head::Node\n",
    "end\n",
    "\n",
    "function list_search(list::LinkedList,key)\n",
    "    x=list.head\n",
    "    while x !=nothing && x.key!=key\n",
    "        x=x.next\n",
    "    end\n",
    "    return x\n",
    "end"
   ]
  }
